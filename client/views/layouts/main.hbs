<body>
    <input type="hidden" name="url" value="{{url}}">
    <header>
        <nav>
            <a class="logo" href="/{{token}}">Blogger Name</a>
            <a href="{{url}}/about/{{token}}">About Me</a>
            <a href="{{url}}/caterogies/{{token}}">Caterogies</a>
            {{#if logged}}
                <a href="{{url}}/admin/{{token}}">Admin Panel</a>
                <div class="right">
                    <a class="btn" href="{{url}}/account/logout/{{token}}">Leave Sesion</a>
                </div>
            {{/if}}
            {{#is title "admin"}}
                <form id="login-form" class="right" method="POST"> 
                    <input type="email" name="email" id="email" placeholder="Email Address">
                    <input type="password" name="pass" id="pass" placeholder="Password">
                    <button onclick="login(event)" class="btn" type="submit">Log in</button>
                </form> 
            {{/is}}
        </nav>
    </header>

